{
  "Name": "OrthancHeadless",
  "HttpServerEnabled": true,
  "HttpPort": 8042,
  "RemoteAccessAllowed": true,
  "AuthenticationEnabled": true,
  "RegisteredUsers": {
    "orthanc": "orthanc"
  },
  "DicomAet": "ORTHANC",
  "DicomPort": 4242,  
  "DicomModalities": {
    "DCMROUTER": [ "DCMROUTER", "172.18.0.8", 11112 ]
  },  
  "DicomWeb": {
    "Enable": true,
    "Root": "/dicom-web/",
    "StowRS": true,
    "WadoRS": true,
    "QidoRS": true
  },
  "HttpServer": {
    "EnableCors": true 
  },
  "OrthancExplorer": {
    "Enable": false
  },
  "StorageDirectory": "/var/lib/orthanc/db"
}
